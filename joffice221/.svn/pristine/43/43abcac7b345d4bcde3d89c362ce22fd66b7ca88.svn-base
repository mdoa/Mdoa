body, html {
    height: 100%;
}
body {
    font-size: 12px;
    margin: -2px 0 0;
    padding: 0;
}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
}
address, caption, cite, code, dfn, em, strong, th, var {
    font-style: normal;
    font-weight: normal;
}
.inputText {
    border: 1px solid #ABC1D1;
    padding: 2px;
}
.readonlyText {
	border: 1px solid #DDD; 
	background-color: #F5F5F5; 
	color:#ACA899; 
	padding: 2px;
}

input, textarea, select {
    font-family: inherit;
    font-size: inherit;
    font-weight: inherit;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}
/** top*/
div.panel-top {
	margin: 2px 5px 0 5px;
}

div.panel-body{
	margin: 2px 5px 0 5px;
}
.table-detail {
	margin: auto;
	width:100%;
}

.table-detail th {
	text-align: right;
	padding-right: 6px;
	color: #000;
	height: 32px;
	border: solid 1px #A8CFEB;
	font-weight: bold;
	text-align: right;
	font-size: 12px;
	font-weight: bold;
	padding-right: 5px;
	background-color: #edf6fc;
	padding-right: 5px;
	border: 1px solid #8dc2e3;
}

.table-detail td {
    border: 1px solid #A8CFEB;
    font-size: 12px;
    padding-left: 6px;
    text-align: left;
}

.head {
	text-align: right;
	padding-right: 6px;
	color: #000;
	height: 32px;
	border: solid 1px #A8CFEB;
	font-weight: bold;
	text-align: right;
	font-size: 13px;
	font-weight: bold;
	padding-right: 5px;
	background-color: #edf6fc;
	padding-right: 5px;
	border: 1px solid #8dc2e3;
}

.tbar-title {
	padding-left: 5px; 
	margin-top: 2px;
	border: 1px #8dc2e3 solid;
	height: 24px;
	padding-top: 5px;
	font-size: 13px;
	font-weight: bold;
	background: url(../images/form/list_title_bg.jpg) repeat-x;
}

.panel-toolbar {
	padding-left: 4px;
	border-bottom: 1px #8dc2e3 solid;
    border-left: 1px #8dc2e3 solid;
    border-right: 1px #8dc2e3 solid;
	height: 24px;
	padding-top: 4px;
	background: #dff5fd;
	

}

.panel-toolbar a {
	text-decoration: none;
	color: blue;
	float: left;
	margin-left: 2px;
}

.group{
	margin-right: 2px;
	margin-left:0px;
	padding-left:0px;
	float:left;
}

.l-bar-separator {
    border-left: 1px solid #9AC6FF;
    border-right: 1px solid white;
    float: left;
    height: 18px;
    margin: 2px;
}

a.link{
	padding-left:20px;cursor: pointer; 
	color: blue;font-size:12px;
	text-decoration: none;
}

a.link.disabled{
	color: gray;
}

a.link:hover{

	text-decoration: underline;
}

a.link.save{
	background:url(../images/form/icons/icons_save.png) 0px -3px no-repeat;
}


a.link.run{
	background: url(../images/form/icons/icons_run.png) 0px 0px no-repeat;
}

a.link.preview{
	background:url(../images/form/icons/icons_preview.png) 0px 0px no-repeat;
}

a.link.colse{
	background:url(../images/form/icons/icons_colse.png) 0px -2px no-repeat;
}
a.link.back{
	background:url(../images/form/icons/icons_back.png) 0px -2px no-repeat;
}


.table-grid {
	border: 1px solid rgb(123, 171, 207);
    width:100%;
}

.table-grid thead {
	height: 32px;
}

.table-grid caption {
	border-width: 1px 1px medium;
	border-style: solid solid none;
	border-color: rgb(168, 207, 235) rgb(168, 207, 235) currentColor;
	height: 14px;
	text-align: left;
	color: rgb(28, 86, 138);
	padding-top: 5px;
	padding-left: 5px;
	font-weight: bold;
	background-color: rgb(223, 234, 251);
}

.table-grid th {
	color: #102A49;
	height: 34px;
	padding-left: 5px;
	text-align: center;
	border: dotted 1px a8cfeb;
	white-space: nowrap;
	border-right: dotted 1px #cacaca;
	background: url(../images/form/content_bg_sort.jpg) repeat-x;
	font-size: 12px;
}

.table-grid td{
	font-size: 12px;
	height: 25px;
}






/** =========================表单 文件==========================================  */
/**
附件样式表。
*/
div.attachement{	
	float:left;
	overflow:hidden;
	padding:3px 0 0 15px;
	white-space:nowrap;		
}

span.attachement-span{
   	float:left;
	background:#f5f6f4;
	border:1px solid #e1e1e1;
	margin:5px 10px 5px 0;
	padding 1px 3px 1px 5px;
} 	  

a.attachment{
	text-decoration: none;
	font-size: 13px;
	color:#606060;
}

a.cancel{
   	background:url(../images/cancel.png) 0 1px no-repeat;
   	display:inline-block;
   	height:14px;
   	margin-left:8px;
   	overflow:hidden;
   	width:14px;
   	text-decoration:none;	
}

a.attachment:hover{
	text-decoration: underline;
}

table.formTable{
	border:1px solid #366092;border-collapse :collapse;width:100%;text-align:center;
	font-size: 12px;
}

td.formHead{
	border:1px solid #366092;
	text-align: center;
	font-size: 16px;
	font-weight: bold;
	background-color: #366092;
	height: 32px;
	color: #fff;
}

.window-table{
	width:50%;
	padding: 0;
    margin: 0;
    margin-top:15px;
    border-collapse:collapse;
}

.window-table th{
	width:40%;
	font-size: 13px;	
	border: 1px solid #A8CFEB;  
    background: #EDF6FC;
    font-size:13px;
    padding: 6px 6px 6px 12px;
}

.window-table td{
	width:60%;
	border: 1px solid #A8CFEB;  
    background: #fff;
    font-size:12px;
    padding: 6px 6px 6px 12px;
}

.blocktable {
	width:100%;
	padding: 0;
    margin: 0;
    margin-top:15px;
    border-collapse:collapse;
}

.blocktable th {
	font-size: 13px;	
	border: 1px solid #A8CFEB;  
    background: #EDF6FC;
    font-size:13px;
    padding: 6px 6px 6px 12px;
}

.blocktable td{
	border: 1px solid #A8CFEB;  
    background: #fff;
    font-size:12px;
    padding: 6px 6px 6px 12px;
}

.datepicker input{
	width:130px;
	background: url(../images/form/icon/datePicker.gif) no-repeat scroll right center #FFFFFF;
    border: 1px solid #999999;
    height: 20px;
}

.dictionary input{
	width:130px;
	background: url(../images/form/icon/dictionary.png) no-repeat scroll right center #FFFFFF;
    border: 1px solid #999999;
    height: 20px;
}

td.formTitle{
	padding:2px;border:1px solid #366092;
	background-color:#EDF6FC;
	text-align: right;
	height: 23px;
}

td.formInput{
	padding:2px;border:1px solid #366092;
	background-color:white;
	text-align: left;
}

table.listTable{
	border:1px solid #366092;border-collapse :collapse;width:100%;text-align:center;
	font-size: 12px;
}

table.listTable tr.headRow{
	height: 25px;
	
}

table.listTable tr.toolBar{
	height: 32px;
	
	background-color: #366092;
}

table.listTable tr.toolBar td{
	border:1px solid #366092;
	text-align: left;
	padding-left: 4px;
}

table.listTable th{
	border:1px solid #366092;
	text-align: center;
	height: 25px;
	font-size:12px;
	background-color:#F1F6FF;
	font-weight:bold;
	
}

/*验证表单字段样式*/
label.error {
	background: url(../images/form/valid/unchecked.gif) no-repeat 0px 0px;
	margin-left: 16px;
	padding-left: 16px;
	padding-bottom: 2px;
	font-weight: bold;
	color: #EA5200;
}

table.listTable tr.listRow td{
	border:1px solid #366092;
