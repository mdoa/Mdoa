.touchgridpanel{
	padding: 0;
	width:100%;
}

.touchgridpanel .x-grid-hd-row {
    background-image : -webkit-gradient(linear, 0% 0%, 0% 100%, color-stop(0%, #9aa7b2), color-stop(2%, #647482), color-stop(100%, #434d57));
}

.touchgridpanel .x-grid-cell {
	text-align   : center !important;
    overflow       : hidden;
    text-overflow  : ellipsis;
	white-space    : nowrap;
	vertical-align : middle;
    min-height     : 2.6em;
	line-height    : 2em;
	font-size      : 1em;
	font-weight    : bold;
    border-left    : 1px solid #ccc;
    border-bottom    : 1px solid #ccc;
}

.touchgridpanel .x-grid-cell-hd {
    text-align   : center !important;
    padding      : 0 !important;
	color        : #fff;
	text-shadow  : rgba(0, 0, 0, 0.5) 0 -0.08em 0;
    border       : 0;
    min-height     : 2.6em;
	line-height    : 2em;
}

.checkedImgUrl {
	border: 3px solid blue;
	float: right;
	width: 28px;
	height: 28px;
	background: url(../images/checkbox_checked.png) no-repeat;
}
.unCheckedImgUrl{
	border: 3px solid green;
	float: right;
	width: 28px;
	height: 28px;
	background:url(../images/checkbox_unchecked.png) no-repeat;
}


.simpleList .x-list-disclosure {
    -webkit-mask-size:100%;
    -webkit-mask-image:url(data:image/png;base64,);
    background-color:#aaa;
    background-image:none;
}
.simpleList .x-item-selected .x-list-disclosure {
    background-color:#00ae9d;
    background-image:none;
}

/*这是是修改List列表 某行被选中时的样式  */
.x-list .x-list-item.x-item-selected .x-dock-horizontal {
    background:#afb4db;
}
@font-face {
	font-family: 'mini';
