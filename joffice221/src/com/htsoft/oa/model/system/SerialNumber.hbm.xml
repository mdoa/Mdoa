    <property name="genType" type="java.lang.Short" update="true" insert="true" access="property" column="GENTYPE" length="22" not-null="false" unique="false"/>  
    <property name="noLength" type="java.lang.Integer" update="true" insert="true" access="property" column="NOLENGTH" length="22" not-null="false" unique="false"/>  
    <property name="curDate" type="java.lang.String" update="true" insert="true" access="property" column="CURDATE" length="10" not-null="false" unique="false"/>  
    <property name="initValue" type="java.lang.Integer" update="true" insert="true" access="property" column="INITVALUE" length="22" not-null="false" unique="false"/>  
    <property name="curValue" type="java.lang.Integer" update="true" insert="true" access="property" column="CURVALUE" length="22" not-null="false" unique="false"/>  
    <property name="step" type="java.lang.Short" update="true" insert="true" access="property" column="STEP" length="22" not-null="false" unique="false"/> 
  </class> 
</hibernate-mapping>
