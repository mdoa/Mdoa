#box{
	width:100%;
	height: 100%;
	background:#d2e1f5;
}
#box>div{float: left;}
#topbar{
	padding-left: 1.3%;
	padding-right: 1.3%;
	width: 100%;
	height: 7.8%;
	color: #8191a8;
	border-bottom: 1px solid #a8c5eb;
	background: url(../../img/index/topbar_bg.jpg);
	position: relative;
}
#topbar #logo{
	float:left;
	padding-top:8px;
}
#topbar #logo img{
	float:left;
}
#topbar #logo #Mendertext{
	margin-left:4px;
}
#topbar .topbarMenu{
	float: left;
	overflow: hidden;
	font-size: 18px; 
	position:absolute;
}
#topbar .topbarMenu li{
	height:100%;
	float: left;
	padding: 0 10px;
	margin-left:10px;
	cursor:pointer;
	color:#5F9EA0;
}
#topbar .topbarMenu .now{
	background: #cddaf8;
}
#topbar .topbarMenu li:hover{
	background: #cddaf8;
}

#topbar .topbarRight{
	float: right;
	text-align:right;
	font-size:14px;
}
#topbar .topbarRight p{
	line-height:22px;
}
#topbar .topbarRight #updatePassword{
	cursor:pointer;
}
#topbar .topbarRight #updatePassword:hover{
	color:#e6779f;
}
#topbar .topbarRight #out{
	text-align:center;
	height:14px;
	width:44px;
	display:inline-block;
	margin-top:4px;
	cursor:pointer;
}
#topbar .topbarRight #out:hover{
	color:#e6779f;
}
#cut{
	width:100%;
	height:10px;
	background:#c7d6f7;
	border-top:1px solid #cddaf8;
}
#leftbar{
	width: 12%;
	height: 87%;
	color:#25292e;
	background:#f4f8fd;
	cursor:pointer;
}
#leftbar .show{
	text-align: left;
}
/* #leftbar .show span{
	display:inline-block;
	height:34px;
	line-height:34px;
} */
#leftbar .show .img_nav{
	float:left;
}
#leftbar .showtop{
	height: 30px;
	background: #81cae7;
	cursor:auto;
}
#leftbar .showtop span{
	letter-spacing: 8px;
}
.navarea h3,h4{
	font-size:18px;
}
.navarea .navpiece h3{
	background: #d3ecfb;
	border: 1px solid #82d3ea;
	border-top:0;
}
navarea h3 img{
	margin-top:1%;
}
.navarea h3 .img_01{
	float:left;
}
.navarea h3 .img_02{
	float:right;
}
.navarea .navpiece>ul{
	height:0px;
	overflow-y:auto;
	border:1px solid #82d3ea;
	border-top:0;
	border-bottom:0;
}
.navarea .on>ul{
	border-bottom:1px solid #82d3ea;
}
.navarea .navpiece ul li{
	text-indent: 0;
}
.navarea .navpiece ul li .treeBg{
	
}
.navarea .navpiece ul li .treeBg{
	height:100%;
	position:relative;
	display:inline-block;
	vertical-align:top;
	margin-right:1px;
}
.navarea .navpiece ul li .treeBg img{
	width:100%;
	height:100%;
	position:absolute;
	left:0px;
	top:0px;
}
.navarea .navpiece .navpiece2>li {
	height:22px;
	line-height:22px;
	font-size:16px;
	padding-left: 2em;
}
.navarea h3{
	font-weight: 700;
}
#main{
	width: 87.5%;
	margin-left: 0.5%;
	background:#fff;
}
#main .maintop{
	height:30px;
	background:#74cad3;
	padding:4px 0;
	padding-left:12px;
	margin-top:1px;
}
#main .maintop>div{
	overflow:hidden;
	float:left;
	border:1px solid #74cad3;
	border-radius: 5px;
	behavior: url(../../css/plug/ie-css3.htc);
	cursor:pointer;
}
#main .maintop>div:hover{
	border:1px solid #78dce6;
	background:#78dce6;
}
#main .maintop span{
	float:left;
	display:inline-block;
	height:22px;
	line-height:22px;
	color:#fff;
	cursor:pointer;
}
#main .maintop .maintopicon{
	width:18px;
}
#main .maintop .mainicon1{
	width:22px;
	background:url(../../img/index/refresh.png) no-repeat 3px 4px;
}
#main .maintop .mainicon2{
	background:url(../../img/index/add.png) no-repeat 3px 4px;
}
#main .maintop .mainicon3{
	background:url(../../img/index/del.png) no-repeat 3px 4px;
}
#main .maintop .mainmanger{
	background:url(../../img/index/iocn3.png) no-repeat 4px 6px;
	background-size:50% 50%;
}
#main #loadarea{
	text-align: left;
	width: 100%;
	height: 100%;
	overflow:hidden;
}
#bottombar{
	width:100%;
	height:3.1%;
	border-bottom:1px solid #fff;
	background:#a3d3eb;
	padding:0 12px;
	position:fixed;
	bottom:0;
	left:0;
}
#bottombar .bottomleft{
	float:left;
	height:100%;
	cursor:pointer;
}
#bottombar .bottomleft #indexMessage{
	float:left;
}
#bottombar .bottomleft span{
	float:left;
	padding:2px;
}
#bottombar .bottomleft img{
	padding:2px;
	float:left;
}
.popup .user{
	padding:0px;
}
/*---------------五金仓库-------------------*/
.treearea{
	height:100%;
	float:left;
	width:200px;
	border:1px solid #ededed;
	border-bottom:none;
	background:#f2f2f2;
	overflow-y:auto;
}
.treearea h3{
	text-align:center;
	height:29px;
	line-height:30px;
	font-size:18px;
	background:#e3e3e3;
	font-weight:600;
}
.listForm{
	float:left;
	height:100%;
}
.listForm .toolbar{
	height: 30px;
	background-color: #66CCCC;
	padding-top: 4px;
	padding-left: 10px;
}
.listForm .queryForm{
	padding-left:10px;
	padding-top: 2px;
	line-height:30px;
}
.listForm .queryForm input{
	width:40px;
}
.listForm .queryForm .time{
	width:100px;
	background:url(../../img/index/jedetabg.png) no-repeat right center #fff;
}
.rightarea{
	float: right;
	width: 300px;
	height:100%;
}
.rightarea .lump{
	height:50%;
}
/*弹窗*/
.popup{
	position:absolute;
	border-radius: 12px;
	behavior: url(../../css/plug/ie-css3.htc);
	background: #e0edff;
	padding:0 12px;
	color:#333;
	border: 1px solid #dedede;
	display: none;
}
.popup h3{
	height: 30px; 
	line-height:30px;
	font-size: 20px;
	font-weight: 600;
	color:#444a6f;
	cursor:move;
}
.popup .turnoff{
	position:absolute;
	right:14px;
	top:6px;
	display:inline-block;
	width:16px;
	height:16px;
	border:1px solid #ccd8e7;
	background:url(../../img/index/turnoff.png) no-repeat 2px 2px;
	border-radius: 4px;
	behavior: url(../../css/plug/ie-css3.htc);
}
.popup .turnoff:hover{
	border:1px solid #a5c4ed;
	background:url(../../img/index/turnoff.png) no-repeat 2px 2px #d6eaff;
}
.popup .popuparea{
	border:1px solid #a3c8f8;
	border-radius: 4px;
	behavior: url(../../css/plug/ie-css3.htc);
	background:#fff;
}
.popup table{
	/* padding:20px 30px; */
}
.popup .popuparea input{
	width:146px;
}
.popup .popuparea select{
	width:150px;
}
.popup table textarea{
	width:98%;
}
.popup table .popuptext{
	padding-bottom:23px;
}
.popup .btnarea{
	height:30px;
	padding:5px 0;
}
.del{
	height:140px;
	width:300px;
}
.del .delarea{
	height:80px;
	line-height:80px;
	color:red;
	text-align:center;
}
.popup .popuparea .title{
	padding:5px;
	height: 27px;
	background-color: #E0ECFF;
	font-size: 12px;
	border-bottom: 1px #95B8E7 solid;
	font-weight: bold;
}
/*---------------------部门盒子-----------------*/
.departmentBox{
	display:inline-block;
	padding:3px 6px;
	position:relative;
	line-height:14px;
	background:#fff;
	border-radius: 4px;
    behavior: url(../../css/plug/ie-css3.htc);
    margin-left:4px;
    border:1px solid #a8a8a8;
}
.departmentBox .turnoff{
	position:absolute;
	top:1px;
	right:1px;
	cursor: pointer;
	width:6px;
	height:6px;
	display:none;
}
/*-----------------消息提示框------------------------*/
.ui_confirm .ui_msg{
	height: 30px;
    line-height: 30px;
    font-size: 20px;
    font-weight: 600;
    color: #444a6f;
    background: #e0edff;
    width:200px;
    text-align: center;
    padding:0;
    margin:0;
}
.ui_confirm .ui_p{
	background: #e0edff;
	padding: 3px 2px 3px;
}  
.ui_p .ui_button{
	color: #333;
	background: url(../../img/index/btnbg1.jpg) repeat-x;
	border:1px solid #ababab;
	border-radius: 4px;
	height:20px;
	line-height: 17px;
}
.ui_p .ui_button:hover{
	color: #333;
	background: url(../../img/index/btnbg1.jpg) repeat-x;
	border:1px solid #ababab;
	border-radius: 4px;
	height:20px;
	line-height: 17px;
}
.ui_p .ui_button:active{
	background:url(../../img/index/btnbg2.jpg) repeat-x;
}
/*-------------------打印弹窗------------------------*/
#print-message table td{
	border: 1px black solid;
}
#print-message .stretchDiv{
	cursor: col-resize;
	width:10px;
	position: absolute;
	top:31px;
}
#print-message .popuparea .print-col-box{
	overflow:hidden;
	height:20px;
}
#print-message #print-col{
	position: absolute;
	display:none;
	z-index:1000;
	top:31px;
	left:40px;
	background:#fff;
	padding-top:9px;
	padding-left:10px:
}
#print-message #print-col input{
	width:13px;
	height:13px;
}
#print-message #print-col span{
	padding:2px 3px;
}
#print-message ul{
	position: absolute;
	display: none;
	background:#fff;
	cursor:pointer;
	z-index:10;
	padding:2px 3px;
	background:#fafafa;
}
#print-message ul li{
	padding:2px 8px;
}
#print-message ul li:hover{
	background:#eaf2ff;
}
